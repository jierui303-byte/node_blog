<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>后台</title>

</head>
<body>
    <h1>admin</h1>

    <form action="/admin/article/addData" method="post">
        <h1>文章新增</h1>
        <p>标题: <input type="text" name="title"><span>标题为主键,唯一</span></p>
        <p>类目:
            <select name="categoryId" id="">
                <% categoryLists.forEach(function(category){ %>
                <option value="<%= category._id %>"><%= category.categoryName %></option>
                <% }) %>
            </select>
        </p>
        <p>描述: <input type="text" name="description"></p>
        <p>作者: <input type="text" name="author"></p>
        <p>内容: <textarea name="content" id="" cols="30" rows="10"></textarea></p>
        <p>标签: <input type="text" name="articleTag"></p>
        <p><input type="submit" value="提交"></p>
    </form>

    <hr/>



</body>
</html>